{
	"$schema": "vscode://schemas/color-theme",
	"name": "VSkript",
	"type": "dark",
	"include": "./dark_plus.json",
	"colors": {
		"editor.background": "#1c1c1c",
		"editor.foreground": "#a0a0a0"
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"aliases": { "foreground": "#1bbba6" },
		"parameter": { "foreground": "#55a8e0", "italic": true }
	},
	"tokenColors": [

		{
			"name": "VSkript Nested",
			"scope": [ "vskript.nested" ],
			"settings": { "foreground": "#a0a0a0", "fontStyle": "" }
		},
		{ "name": "Vskript COlor 0", "scope": [ "vskript.color.0" ], "settings": { "foreground": "#000000", "fontStyle": "" } },
		{ "name": "Vskript COlor 1", "scope": [ "vskript.color.1" ], "settings": { "foreground": "#0000AA", "fontStyle": "" } },
		{ "name": "Vskript COlor 2", "scope": [ "vskript.color.2" ], "settings": { "foreground": "#00AA00", "fontStyle": "" } },
		{ "name": "Vskript COlor 3", "scope": [ "vskript.color.3" ], "settings": { "foreground": "#00AAAA", "fontStyle": "" } },
		{ "name": "Vskript COlor 4", "scope": [ "vskript.color.4" ], "settings": { "foreground": "#AA0000", "fontStyle": "" } },
		{ "name": "Vskript COlor 5", "scope": [ "vskript.color.5" ], "settings": { "foreground": "#AA00AA", "fontStyle": "" } },
		{ "name": "Vskript COlor 6", "scope": [ "vskript.color.6" ], "settings": { "foreground": "#FFAA00", "fontStyle": "" } },
		{ "name": "Vskript COlor 7", "scope": [ "vskript.color.7" ], "settings": { "foreground": "#AAAAAA", "fontStyle": "" } },
		{ "name": "Vskript COlor 8", "scope": [ "vskript.color.8" ], "settings": { "foreground": "#555555", "fontStyle": "" } },
		{ "name": "Vskript COlor 9", "scope": [ "vskript.color.9" ], "settings": { "foreground": "#5555FF", "fontStyle": "" } },
		{ "name": "Vskript COlor A", "scope": [ "vskript.color.a" ], "settings": { "foreground": "#55FF55", "fontStyle": "" } },
		{ "name": "Vskript COlor B", "scope": [ "vskript.color.b" ], "settings": { "foreground": "#55FFFF", "fontStyle": "" } },
		{ "name": "Vskript COlor C", "scope": [ "vskript.color.c" ], "settings": { "foreground": "#FF5555", "fontStyle": "" } },
		{ "name": "Vskript COlor D", "scope": [ "vskript.color.d" ], "settings": { "foreground": "#FF55FF", "fontStyle": "" } },
		{ "name": "Vskript COlor E", "scope": [ "vskript.color.e" ], "settings": { "foreground": "#FFFF55", "fontStyle": "" } },
		{ "name": "Vskript COlor F", "scope": [ "vskript.color.f" ], "settings": { "foreground": "#FFFFFF", "fontStyle": "" } },
			
		{
			"name": "VSkript Comment",
			"scope": [ "vskript.comment" ],
			"settings": { "foreground": "#606060", "fontStyle": "" }
		},
		{
			"name": "VSkript Docs",
			"scope": [ "vskript.docs" ],
			"settings": { "foreground": "#d0d0d0", "fontStyle": "" }
		},
		{
			"name": "VSkript Docs Annotation",
			"scope": [ "vskript.docs.annotation" ],
			"settings": { "foreground": "#9cdcfe", "fontStyle": "" }
		},
		{
			"name": "VSkript Docs Parameter",
			"scope": [ "vskript.docs.parm" ],
			"settings": { "foreground": "#55a8e0", "fontStyle": "" }
		},



		{
			"name": "VSkript Options",
			"scope": "vskript.options",
			"settings": { "foreground": "#ff2e97", "fontStyle": "bold" }
		},
		{
			"name": "VSkript Options Variable ",
			"scope": "vskript.options.variable",
			"settings": { "foreground": "#d15593", "fontStyle": "" }
		},



		{
			"name": "VSkript Aliases",
			"scope": "vskript.aliases",
			"settings": { "foreground": "#35e4cc", "fontStyle": "bold" }
		},
		{
			"name": "VSkript Aliases",
			"scope": [
				"vskript.aliases.variable",
				"vskript.expr.minecraftitem"
			],
			"settings": { "foreground": "#1bbba6", "fontStyle": "" }
		},
		{
			"name": "VSkript Type",
			"scope": [ "vskript.type" ],
			"settings": { "foreground": "#89ddff", "fontStyle": "" }
		},



		{
			"name": "VSkript Event",
			"scope": ["vskript.event"],
			"settings": { "foreground": "#f8ef74", "fontStyle": "bold" }
		},
		{
			"name": "VSkript Command",
			"scope": ["vskript.command"],
			"settings": { "foreground": "#f8ef74", "fontStyle": "bold" }
		},
		{
			"name": "VSkript Command Label & Options",
			"scope": [
				"vskript.command.label",
				"vskript.command.options"
			],
			"settings": { "foreground": "#d1b854", "fontStyle": "" }
		},



		{
			"name": "VSkript Function Keyword",
			"scope": [ "vskript.function.keyword" ],
			"settings": { "foreground": "#b86fe9", "fontStyle": "bold" }
		},
		{
			"name": "VSkript Function Name",
			"scope": [ "vskript.function.name" ],
			"settings": { "foreground": "#9f5ccc", "fontStyle": "" }
		},
		{
			"name": "VSkript Function Parameter",
			"scope": [ "vskript.function.parameter" ],
			"settings": { "foreground": "#55a8e0", "fontStyle": "italic" }
		},
		{
			"name": "VSkript Expression Function",
			"scope": "vskript.expr.function",
			"settings": { "foreground": "#9f5ccc", "fontStyle": "" }
		},



		{
			"name": "VSkript Grammar",
			"scope":  "vskript.grammar",
			"settings": { "foreground": "#f58a4d", "fontStyle": "bold" }
		},
		{
			"name": "VSkript Expression Grammar",
			"scope": "vskript.expr.grammar",
			"settings": { "fontStyle": "italic" }
		},
		{
			"name": "VSkript Expression Number",
			"scope": "vskript.expr.number",
			"settings": { "foreground": "#d0d0d0", "fontStyle": "" }
		},


		
		{
			"name": "VSkript Expression Text",
			"scope": "vskript.expr.text",
			"settings": { "foreground": "#6c9662", "fontStyle": "" }
		},

		
		{
			"name": "VSkript Variable Local",
			"scope": "vskript.variable.local",
			"settings": { "foreground": "#4a84bb", "fontStyle": "" }
		},
		{
			"name": "VSkript Variable Normal, Runtime",
			"scope": [
				"vskript.variable.normal",
				"vskript.variable.runtime"
			],
			"settings": { "foreground": "#4a84bb", "fontStyle": "" }
		},
		{
			"name": "VSkript Variable Backet",
			"scope": [
				"vskript.variable.bracket"
			],
			"settings": { "foreground": "#4a84bb", "fontStyle": "" }
		}
	]
}